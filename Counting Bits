vector<int> countBits(int num) {
        vector<int> result;
        if (num < 0) return result;
        for (int i=0; i<=num; ++i){
            int sum = 0;
            int n = i;
            while(n){
                n = n&(n-1);
                sum++;
            }
            result.push_back(sum);
        }
        return result;
    }
